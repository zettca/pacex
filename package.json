    "@testing-library/react": "^14.0.0",
    "i18next": "^21.8.16",
    "i18next-browser-languagedetector": "^6.1.0",
    "i18next-http-backend": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.8.12",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
